<div id="consultation_modal_franchise" tabindex="-1" role="dialog"  aria-hidden="true"
     class="modal fade">
    <div role="document" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header text-center align-items-center justify-content-center">
                <table class="w100p">
                    <tr>
                        <td>
                            <button type="button" data-dismiss="modal" aria-label="Close" class="close float-right"><span aria-hidden="true"></span></button>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <span class="h3 modal-title  text-center align-items-center justify-content-center "><span>Заказать обратный<br> звонок</span></s
                        </td>
                    </tr>
                </table>
            </div>

            <div class="modal-body form-inline  text-center align-items-center justify-content-center">
                <form id="consultation_form_modal_franchise" action="#"
                      class="form text-center align-items-center justify-content-center" method="post">

                    <input type="hidden" name="event" value="callback_modal_form">
                    <input type="hidden" name="type" value="franchise">
                    {% if geo is defined %}
                        <input type="hidden" name="geo" value="{{ geo }}">
                        <input type="hidden" name="host" value="{{ geo.host }}">
                    {% endif %}


                    <div class="form-group">
                        <input type="text" name="name" placeholder="Ваше имя" required pattern="[A-zА-я]{3,}[A-zА-я-\s]*" >

                        <input  type="text" name="phone" pattern="[\+]*[0-9]+" placeholder="Ваш телефон" required>
                        <button type="submit" class="mt-3 btn-lg btn-success submit">Заказать звонок</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


