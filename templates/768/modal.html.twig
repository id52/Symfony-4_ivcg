<div id="consultation_modal_3" tabindex="-1" role="dialog"  aria-hidden="true"
     class="modal fade">
    <div role="document" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header text-center align-items-center justify-content-center">
                <table class="w100p">
                    <tr>
                        <td>
                            <button type="button" data-dismiss="modal" aria-label="Close" class="close float-right"><span aria-hidden="true"></span></button>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <span class="h3 modal-title  text-center align-items-center justify-content-center "><span>Присоединиться</span></span>
                        </td>
                    </tr>
                </table>
            </div>


            <div class="modal-body form-inline text-center align-items-center justify-content-center">
                <form id="consultation_form_modal_3" action="#" enctype="multipart/form-data"
                      class="form text-center align-items-center justify-content-center" method="post">

                    <input type="hidden" name="event" value="join_modal_form">
                    <input type="hidden" name="type" value="join">
                    {% if geo is defined %}
                        <input type="hidden" name="geo" value="{{ geo }}">
                        <input type="hidden" name="host" value="{{ geo.host }}">
                    {% endif %}

                    <div class="form-group">
                        <input type="text" name="name" required placeholder="Ваше имя">

                        <input type="text" name="phone" required pattern="[\+]*[0-9]+" placeholder="Ваш телефон">

                        <label class="mt-2 modal-label-passport">
                            Ваша фотография с паспортом
                        </label>

                        <div class="mt-1 text-danger" id="join-error-size">
                            Размер файла должен быть меньше 5 Мб
                        </div>

                        <div class="mt-1 text-danger" id="join-error-type">
                            Допустимые форматы файла: jpeg, png или gif
                        </div>

                        <label class="mt-2 btn-lg btn-primary btn-shadow text-white">
                            <span class="pl-5">Загрузить файл</span>
                            <input id="join-upload-file" type="file" hidden required name="photo" accept="image/jpeg,image/png,image/gif">
                        </label>

                        <label class="text-primary" id="join-attached-file">Файл не выбран</label>

                        <textarea rows="4"  class="form-control w-100 mt-3" name="commentary" placeholder="Комментарий"
                                  required></textarea>
                        <button type="submit" class="mt-4 btn-lg btn-success submit"><span>Присоединиться</span></button>
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>

