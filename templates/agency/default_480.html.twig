{% extends '480/layout.html.twig' %}

{% block title %}
    {% if app.request.host == 'ivcg.ru' %}
        {{ geo.headTitleAgency }}
    {% else %}
        {{ geo.headTitleSubdomainAgency|geo_prepositon(geo.getPrepositionalCaseWithPrepositionLow()) }}
    {% endif %}
{% endblock %}

{% block description %}
    {% if app.request.host == 'ivcg.ru' %}
        {{ geo.metaDescriptionAgency }}
    {% else %}
        {{ geo.metaDescriptionSubdomainAgency|geo_prepositon(geo.getPrepositionalCaseWithPrepositionLow()) }}
    {% endif %}
{% endblock %}

{% block content %}
    {% include '480/menu.html.twig' %}
    {% include '480/modal.html.twig' %}
    {% include 'agency/480/intro.html.twig' %}
    {% include 'agency/480/consultation_form_agency.html.twig' %}
    {% include 'agency/480/three_rectangles.html.twig' %}
    {% include 'agency/480/intro2.html.twig' %}
    {% include 'agency/480/list.html.twig' %}
    {% include 'agency/480/three_squares.html.twig' %}
    {% include 'agency/480/agencies.html.twig' %}
    {% include 'agency/480/intro3.html.twig' %}
    {% include '480/footer.html.twig' %}
{% endblock content %}
