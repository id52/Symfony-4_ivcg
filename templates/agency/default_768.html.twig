{% extends '768/layout.html.twig' %}

{% block title %}
    {% if app.request.host == 'ivcg.ru' %}
        {{ geo.headTitleAgency }}
    {% else %}
        {{ geo.headTitleSubdomainAgency|geo_prepositon(geo.getPrepositionalCaseWithPrepositionLow()) }}
    {% endif %}
{% endblock %}

{% block description %}
    {% if app.request.host == 'ivcg.ru' %}
        {{ geo.metaDescriptionAgency }}
    {% else %}
        {{ geo.metaDescriptionSubdomainAgency|geo_prepositon(geo.getPrepositionalCaseWithPrepositionLow()) }}
    {% endif %}
{% endblock %}

{% block content %}
    {% include '768/menu.html.twig' %}
    {% include '768/modal.html.twig' %}
    {% include 'agency/768/intro.html.twig' %}
    {% include 'agency/768/consultation_form_agency.html.twig' %}
    {% include 'agency/768/three_rectangles.html.twig' %}
    {% include 'agency/768/intro2.html.twig' %}
    {% include 'agency/768/list.html.twig' %}
    {% include 'agency/768/three_squares.html.twig' %}
    {% include 'agency/768/agencies.html.twig' %}
    {% include 'agency/768/intro3.html.twig' %}
    {% include '768/footer.html.twig' %}
{% endblock content %}
