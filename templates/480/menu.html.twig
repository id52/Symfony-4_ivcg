<div id="menu" class="justify-content-center text-center align-content-center px-5">
    <div class="container px-0 py-0">
        <table width="100%">
            <tr>
                <td class="px-0 valign-top pt-0">
                    <table class="px-2 mb-2 mx-0" width="auto">
                        <tr>
                            <td class="text-left px-3 {% if app.request.attributes.get('_route') =='agency' %}bg-current-menu{% endif %}">
                                <a href="{{ path('agency') }}"
                                   class="px-0 nav-link link-scroll
                                    {% if app.request.attributes.get('_route') =='agency' %}
                                        text-white
                                    {% else %}
                                        text-dark
                                    {% endif %}
                                ">

                                    <span class="mr-1">
                                        {% if app.request.attributes.get('_route') == 'agency' %}
                                            <img alt="" src="/img/1x1.png" data-src="/img/icons/city_white.png"
                                                 class="img-fluid icon">
                                        {% else %}
                                            <img alt="" src="/img/1x1.png" data-src="/img/icons/city.png"
                                                 class="img-fluid icon">
                                        {% endif %}
                                        Агенствам
                                    </span>

                                </a>
                            </td>
                            <td class="text-left px-3  {% if app.request.attributes.get('_route') =='organization' %}bg-current-menu{% endif %}">
                                <a href="{{ path('organization') }}"
                                   class="px-0 nav-link link-scroll
                                   {% if app.request.attributes.get('_route') =='organization' %}
                                   text-white
                                    {% else %}
                                    text-dark
                                    {% endif %}
                                    ">
                                <span class="">
                                    <img alt="" src="/img/1x1.png"
                                         data-src="{% if app.request.attributes.get('_route') =='organization' %}/img/icons/agent_white.png{% else %}/img/icons/agent.png{% endif %}"
                                         class="img-fluid icon">
                                    Организациям
                                </span>

                                </a>
                            </td>
                            <td class="text-left ">
                                <a href="#" data-toggle="modal" data-target="#consultation_modal_1"
                                   class="px-0 nav-link link-scroll text-dark">
                            <span class=""><img alt="" src="/img/1x1.png" data-src="/img/icons/franchise.png"
                                                class="img-fluid icon ml-3">Франшиза</span>

                                </a>
                            </td>


                        </tr>

                    </table>
                </td>

                <td>
                    <ul class="list-inline p-0">
                        {% if geo is defined %}
                            <li class="list-inline-item geo-city-li" id="your-city">
                                <a href="#" class="nav-link link-scroll text-dark">
                                <span class="icon">
                                    <img alt="" src="/img/icons/point.png" class="img-fluid">
                                </span>
                                </a>

                            </li>

                            <li class="list-inline-item menu-has-children geo-city-li">
                                <a href="#" class="nav-link link-scroll text-dark">
                                <span class="icon" id="geo_city">

                                    {{ geo.city }}
                                    <img alt="" src="/img/angle_down.png" class="img-fluid">
                                </span>
                                </a>
                            </li>
                        {% endif %}
                    </ul>
                </td>


                <td class="mx-0 px-0 w10px">
                    <a href="https://vk.com/visoviycenter" class="nav-link link-scroll text-right pr-0 mr-0 mb-3">
                        <span class="icon "><img alt="" src="/img/1x1.png" data-src="/img/icons/vk.png"></span>
                    </a>
                </td>
            </tr>
        </table>

    </div><!-- //.container -->
</div>


<div class="container-fluid">
    <div class="row">
        <div class="col"><br>
            <hr id="menu-hr">
        </div>
    </div>
</div><!-- //.container-fluid -->


<div id="menu_container_3" class="justify-content-center text-center align-content-center px-5 my-3 my-0">
    <div class="container px-0">
        <table class="" width="100%">
            <tr>
                <td>
                    <table>
                        <tr>
                            <td width="50%">
                                <div id="logo" class="float-left mx-0 px-0">
                                    <a href="/"><img alt="" src="/img/logo2.png" height="65px" class="mx-0 px-0"></a>
                                </div>
                            </td>
                        </tr>
                    </table>
                </td>

                <td class="w250px text-right">
                    <div id="phone">
                    <span class="text-dark">
                            <a href="tel:{{ geo.phone }}" class="text-dark phone-link" id="menu-phone">
                                <span class="phone_roistat_{{ geo.host }} phone_2gis_{{ geo.host }} phone_straight_{{ geo.host }}">{{ geo.phone }}</span>
                            </a>
                    </span>
                        <br>
                        <a href="#" data-toggle="modal" data-target="#consultation_modal_1" class="u order-call">Заказать обратный
                            звонок</a>
                    </div><!--//phone-->
                </td>

                <td class="w10px">
                    <span class="icon float-right">
                        <img alt="" src="/img/1x1.png" data-src="/img/icons/menu_menu.png"
                             class="menu-right-icon show-menu-modal href">
                    </span>
                </td>


            </tr>
        </table>

        {% if geos is defined %}

            <div id="city-footer-hideable" class="text-left align-content-center justify-content-center">

                <input type="text" class="mb-1">

                {% for geo in geos %}
                    {% if geo.city=='Россия' %}
                        <span><a class="set-geo-id-cookie text-left mt-1 ml-3" data-geo-id="{{ geo.id }}"
                                 href="https://{{ geo.host }}/">{{ geo }}</a><br></span>
                        <hr class="mt-2 mb-0 mx-3">
                    {% else %}
                        <span><a class="set-geo-id-cookie text-left mt-1 ml-4" data-geo-id="{{ geo.id }}"
                                 href="https://{{ geo.host }}/">{{ geo }}</a></span><br>
                    {% endif %}
                {% endfor %}
            </div><!--//city-footer-hideable-->
        {% endif %}
    </div>
</div>