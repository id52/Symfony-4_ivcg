{% extends '768/layout.html.twig' %}



{% block title %}
    {% if app.request.host == 'ivcg.ru' %}
        {{ geo.headTitleOrganization }}
    {% else %}
        {{ geo.headTitleSubdomainOrganization|geo_prepositon(geo.getPrepositionalCaseWithPrepositionLow()) }}
    {% endif %}
{% endblock %}

{% block description %}
    {% if app.request.host == 'ivcg.ru' %}
        {{ geo.metaDescriptionOrganization }}
    {% else %}
        {{ geo.metaDescriptionSubdomainOrganization|geo_prepositon(geo.getPrepositionalCaseWithPrepositionLow()) }}
    {% endif %}
{% endblock %}


{% block content %}
